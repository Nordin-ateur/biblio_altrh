<form method="post" action="{{ path('app_admin_' ~ entite ~ '_delete', {'id': entite.id}) }}" 
        onsubmit="return confirm('Confirmer la suppression ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ entite.id) }}">
    <button class="btn btn-danger"><i class="fa fa-trash"></i> Supprimer</button>
</form>
